    <style name="WrapMatch.TableMenuText">
        <item name="android:paddingStart">@dimen/len_6</item>
        <item name="android:paddingEnd">@dimen/len_6</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/font_20</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="MatchOne">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
    </style>

    <style name="OneMatch">
        <item name="android:layout_width">1px</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="WeightMatch">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="MainCommonUi">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/common_ui_middle_size</item>
        <item name="layout_constraintLeft_toLeftOf">parent</item>
        <item name="android:textSize">@dimen/text_middle_size</item>
        <item name="android:textColor">@color/textPrimary</item>
        <item name="android:layout_marginStart">@dimen/len_20</item>
        <item name="android:layout_marginEnd">@dimen/len_20</item>
    </style>

    <style name="CommonToolbar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/common_ui_big_size</item>
        <item name="android:background">@color/colorPrimary</item>
        <item name="android:elevation">@dimen/len_4</item>
        <item name="android:outlineProvider">background</item>
        <item name="titleTextColor">@color/white</item>
    </style>

    <style name="WrapWrap.TextViewNormalStyle">
        <item name="android:padding">6dp</item>
        <item name="android:textColor">@color/textPrimary</item>
        <item name="android:gravity">center</item>
        <item name="android:drawablePadding">@dimen/len_4</item>
    </style>

    <style name="WrapWrap.TextViewNormalStyle.Normal">
        <item name="android:textSize">@dimen/text_middle_size</item>
    </style>

    <style name="WrapWrap.TextViewNormalStyle.Larger">
        <item name="android:textSize">@dimen/text_big_size</item>
    </style>

    <style name="WrapWrap.TextViewNormalStyle.Title">
        <item name="android:textSize">20sp</item>
        <item name="android:padding">20dp</item>
    </style>

    <style name="TabLayoutStyle" parent="Base.Widget.Design.TabLayout">
        <item name="tabBackground">@color/white</item>
        <item name="tabIndicatorColor">@color/colorPrimary</item>
        <item name="tabIndicatorHeight">1dp</item>
        <item name="tabSelectedTextColor">@color/colorPrimary</item>
    </style>

    <style name="CustomStyle" parent="Base.Widget.Design.TabLayout">
        <item name="android:textSize">@dimen/text_large_size</item>
        <item name="android:textColor">@color/textPrimary</item>
    </style>

</resources>
